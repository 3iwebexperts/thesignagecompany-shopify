<div class="projects_list">
  <div class="page-width pad80">
    <div class="proj_head">
      {% if section.settings.pro_heading != blank %}
        <h2>{{ section.settings.pro_heading }}</h2>
      {% endif %}
      {% if section.settings.pro_content != blank %}
        <p>{{ section.settings.pro_content }}</p>
      {% endif %}
    </div>

    <div class="dview">
      <div class="projects_inner">
        <div class="column_one">
          {% for block in section.blocks %}
            {% if block.type == 'image1' %}
              <div class="proj_item">
                <div class="overlay">
                  <div class="proj_info">
                    {% if block.settings.bheading != blank %}
                      <h4>{{ block.settings.bheading }}</h4>
                    {% endif %}

                    {% if block.settings.bheading2 != blank %}
                      <p>{{ block.settings.bheading2 }}</p>
                    {% endif %}
                  </div>
                  {% if block.settings.btnname != blank %}
                    <a class=""><img src="{{ 'mclick.svg' | asset_url }}">{{ block.settings.btnname }} </a>
                    
                  {% endif %}
                </div>
                <a
                  data-fancybox="projgallery"
                  data-caption="{{ block.settings.bheading }}<br>{{ block.settings.bheading2 }}"
                  href="{{ block.settings.image_1 | img_url:'530x' }}"
                >
                  <div class="img_item">
                    {% if block.settings.image_1 != blank %}
                      <div class="backimg" style="background-image:url({{ block.settings.image_1 | img_url:'530x' }});">
                        <svg
                          class="transparent_svg"
                          data-name="Layer 1"
                          xmlns="http://www.w3.org/2000/svg"
                          viewBox="0 0 531 651"
                        >
                          <rect class="cls-1" x="0.5" y="0.5" width="530" height="650"></rect>
                        </svg>
                      </div>
                    {% endif %}
                  </div>
                </a>
              </div>
            {% endif %}
          {% endfor %}
