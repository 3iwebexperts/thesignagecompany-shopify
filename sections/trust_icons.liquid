 
  <div class="trust_icons">
    <div class="page-width">
      <div class="inner_trust swiper">  
        <div class="ticons_list swiper-wrapper">
          {% for block in section.blocks %}
            <div class="ticons_item swiper-slide">
              {% if block.settings.icon1 %}
                <div class="tr_icon">
                  <img src="{{ block.settings.icon1 | img_url:'master' }}" width="" height="">
                </div>
              {% endif %}
              {% if block.settings.heading1 %}
                <div class="tr_txt">
                  <p>{{ block.settings.heading1 }}</p>
                </div>
              {% endif %}
            </div>
          {% endfor %}
        </div>
      </div>
    </div>
  </div>
 

{% schema %}
{
  "name": "Trust Icons",
   "class": "trust-icns",
   "tag":"section",
  "settings":[],
   "blocks": [
         {
          "name": "Annoucement",
          "type": "Annoucement",
          "settings": [
            {
              "type": "image_picker",
              "id": "icon1",
              "label": "Icon"
            },
            {
              "type": "text",
              "id": "heading1",
              "label": "Heading"
            }
          ]
        }
 ],
  "presets": [
    {
      "name": "Trust Icons",
      "category": "Images"
    }
  ]
}
{% endschema %}

