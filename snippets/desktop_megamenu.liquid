 
<div class="page-width custom_mega_menu">  
    
  {% assign formega = settings.formegamenu  %}
  <!-- start left side -->
  <div class="left_side">
    <!-- start all menu items -->
    <div class="mmenu_items">
      {%- for childlink in link.links -%}
      <div class="mmenu_item">
        <a class="{% if forloop.index == 1 %} active {% endif %}" href="#tab{{ forloop.index }}"> {{ childlink.title }}</a>
      </div>
    {% endfor %}   
    </div>
  
    <!-- end all menu items -->
  </div>
  <!-- end left side -->

  <!-- start right side -->
  <div class="right_side deskside">
    <!-- start right item -->
  {% for block in section.blocks %}
    <div id="tab{{ forloop.index }}" class="tab_inner right_item" {% if forloop.index == 1 %} style="display:block; "{% endif %}>
      <!-- start slider items -->
      <div class="slider_items swiper">
        <!-- start slider inner -->
        <div class="slider_inner swiper-wrapper">
          <!-- start slider item --> 
          {% if  block.settings.megaimage1 != blank %}        
            <div class="slider_item swiper-slide">
              <a href="{{ block.settings.megalink1 }}">
                <div class="slide_data"> 
                  <div class="slide_img">
                    <div
                      class="backimg visible"
                      style="background-image:url({{ block.settings.megaimage1 | img_url:'master' }});"
                    >
                      <svg
                        class="transparent_svg"
                        data-name="Layer 1"
                        xmlns="http://www.w3.org/2000/svg"
                        viewBox="0 0 531 351"
                      >
                        <rect class="cls-1" x="0.5" y="0.5" width="530" height="350"></rect>
                      </svg>
                    </div>
                  </div>
                 {% if  block.settings.megasubheading1 != blank %} 
                  <div class="slide_subtt">
                    <p>{{ block.settings.megasubheading1 }} </p>
                  </div>
                 {% endif %} 
                 {% if  block.settings.megaheading1 != blank %}    
                  <div class="slide_head">
                    <h5> {{ block.settings.megaheading1 }} </h5>
                  </div>
                  {% endif %} 
                </div>
              </a>
            </div>
          {% endif %}    
            <!-- end slider item -->
<!-- start slider item -->  
